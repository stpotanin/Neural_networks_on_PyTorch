      "source": [
        "import torch\n",
        "\n",
        "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
        "targets = torch.rand(1000).to(device)\n",
        "predict = torch.rand(1000).to(device)\n",
        "Q = ((predict - targets)**2).mean().cpu()\n",
        "Q.device"
      ]
